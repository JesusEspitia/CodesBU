@model IEnumerable<Monotoring.Models.WorkOrden>

@{
    ViewBag.Title = "Generate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="page-header">
    <h1>Ordenes nuevas</h1>
</div>
<div class="container">
    <div class="row">
        <div class="col-sm-4 col-md-offset-8">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h2 class="panel-title">Actualizar ordenes</h2>
                </div>
                <div class="panel-body">
                    @using (Html.BeginForm("Update", "WorkOrden", FormMethod.Post, new { enctype = "multipart/form-data" }))
                    {
                        <label for="fileUp">Seleccione el archivo: </label>
                        <input type="file" class="" id="fileUp" name="fileUp" />
                        <br />
                        <input type="submit" class="btn btn-default" value="Actualizar" />
                    }
                </div>
            </div>
        </div> 
    </div>

    <div class="row">
        <table class="table table-hover table-condensed">
            <thead>
                <tr>
                    <th class="text-center">
                        Producto
                    </th>
                    <th class="text-center">
                        @Html.DisplayNameFor(model => model.BatchOrden)
                    </th>
                    <th class="text-center">
                        @Html.DisplayNameFor(model => model.dateRegistry)
                    </th>
                    <th class="text-center">
                        @Html.DisplayNameFor(model => model.quantityOrden)
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
            {
                    <tr>
                        <td class="text-center">
                            @item.Catalog.CatalogNo / @item.Catalog.CatalogDescrip
                        </td>
                        <td class="text-center">
                            @item.BatchOrden
                        </td>
                        <td class="text-center">
                            @item.dateRegistry
                        </td>
                        <td class="text-center">
                            @item.quantityOrden
                        </td>
                    </tr>
                }
            </tbody>

        </table>
    </div>
</div>