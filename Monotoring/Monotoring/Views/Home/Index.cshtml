<dvi class="container-fluid">
    <div class="table-responsive">
        <table class="table table-bordered table-hover">
            <tr>
                <th>
                    Orden
                </th>
                <th>
                    Ensamble Final
                </th>
                <th>
                    Calidad
                </th>
                <th>
                    Lógistica
                </th>
                <th>
                    Esterilización
                </th>
                <th>
                    Liberación QA Final
                </th>
                <th>
                    Envió P/L
                </th>
            </tr>
            @foreach(WorkOrden w in Model.WorkOrden)
            {
                <tr>
                    <td>@w.BatchOrden</td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
            }
        </table>
    </div>
</dvi>









